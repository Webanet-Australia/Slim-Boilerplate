{
    "name": "webanet-australia/slim-boilerplate-sm-admin-2-dashboard",
    "description": "Slim PHP Micro-Framework skeleton with SB Admin2 Dashboard from startbootstrap.com",
    "keywords": ["slim", "eloquent", "twig", "bootstrap 4", "framework", "boilerplate", "skeleton", "Dashboard", "SB Admin 2", "Slim Start Bootstrap"],
    "homepage": "https://github.com/Webanet-Australia/Slim-Boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexis Wurth",
            "email": "awurth.dev@gmail.com",
            "homepage": "https://github.com/awurth"
        },
        {
            "name" : "David Miller",
            "homepage": "http://davidmiller.io/"
        },
        {
            "name" : "Isaac Perkins",
            "homepage" : "http://webanet.com/au/",
            "email" : "zak@webanet.com.au"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "awurth/slim-helpers": "^1.0",
        "awurth/slim-validation": "^3.0",
        "cartalyst/sentinel": "^2.0",
        "illuminate/database": "^5.4",
        "illuminate/events": "^5.4",
        "monolog/monolog": "^1.23",
        "respect/validation": "^1.1",
        "slim/csrf": "^0.8.0",
        "slim/flash": "^0.2.0",
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "symfony/console": "^3.0|^4.0",
        "symfony/dotenv": "^3.0|^4.0",
        "symfony/http-foundation": "^3.0|^4.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.6.1",
        "phpunit/phpunit": "^4.8|^5.0|^6.0",
        "symfony/var-dumper": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8080 -t public",
        "test": "phpunit"
    }
}
